;(function($){
  $.fn.datetimepicker.dates['<%= current_locale %>'] = {
    days: <%=raw Date::DAYNAMES.map(&:downcase).map{|d| t :"common.days.#{d}" }.inspect %>,
    daysShort: <%=raw Date::ABBR_DAYNAMES.map(&:downcase).map{|d| t :"common.abbr_days.#{d}" }.inspect %>,
    daysMin: <%=raw %W{su mo tu we th fr sa}.map{|d| t :"common.tiny_days.#{d}" }.inspect %>,
    months: <%=raw Date::MONTHNAMES.compact.map(&:downcase).map{|m| t :"common.months.#{m}" }.inspect %>,
    monthsShort: <%=raw Date::ABBR_MONTHNAMES.compact.map(&:downcase).map{|m| t :"common.abbr_months.#{m}" }.inspect %>,
    today: <%=raw t(:'common.today').inspect %>,
    suffix: [],
    meridiem: [],
    weekStart: 1,
    format: 'yyyy-mm-dd hh:ii'
  };

  $('.form_datetime').datetimepicker({ fontAwesome: true, minView: 1, todayBtn: false, autoclose: true, todayHighlight: false, startView: 2, startDate: new Date(), forceParse: false});
}(jQuery));

