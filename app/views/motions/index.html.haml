%ul
  #closed-motion-list
    - if @closed_motions
      - @closed_motions.each do |motion|
        %li= render '/motions/motion_preview', motion: motion, this_group: @group
    - else
      %li.no-proposals= "There are no closed proposals"
  #closed-motions-loading
    %li.loading Loading...
= paginate @closed_motions, :window => 3, :remote => :true
