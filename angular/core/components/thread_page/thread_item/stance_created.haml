.stance-created
  %poll_common_stance_choice{ng-repeat: "choice in eventable.stanceChoices()", ng-if: "choice.score > 0", stance_choice: "choice"}
  .lmo-hint-text{ng-if: "eventable.stanceChoices().length == 0", translate: "poll_common_votes_panel.none_of_the_above"}
  .lmo-markdown-wrapper{marked: "eventable.reason", ng-if: "eventable.reason"}
  .lmo-md-actions
    %reactions_display{model: "eventable"}
    %action_dock{model: "eventable", actions: "actions"}
