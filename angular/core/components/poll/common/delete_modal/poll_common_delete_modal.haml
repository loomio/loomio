%md-dialog.poll-common-delete-modal
  %md-toolbar
    .md-toolbar-tools.lmo-flex__space-between
      %h1.lmo-h1{translate: "poll_common_delete_modal.title"}
      %material_modal_header_cancel_button

  %md-dialog-content.md-dialog-content
    %p{translate: "poll_common_delete_modal.question"}
    .lmo-md-actions
      %md-button{ng-click: "$close()", translate: "common.action.cancel"}
      %md-button.md-warn.md-raised{ng-click: "submit()", translate: "common.action.delete"}
