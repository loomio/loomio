- set_title @group.name, "New Proposal"
#new-motion
  = render '/groups/title', group: @group
  %h2.new-title New proposal
  = render 'motions/form', form_args: [@motion, {url: motions_path(group_id: @motion.group)}],
    submit_text: "Create proposal", :data => { :disable_with => "Creating proposal" }
